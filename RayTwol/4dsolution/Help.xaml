<Window x:Class="RayTwol._4dsolution.Help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RayTwol._4dsolution"
        mc:Ignorable="d"
        Title="Help" Height="300" Width="460">
    <Grid Background="#FF111617">
        <RichTextBox x:Name="richTextBox" Margin="127,0,0,0" Background="#FF111617" BorderBrush="#FF1E4139" IsEnabled="False">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
